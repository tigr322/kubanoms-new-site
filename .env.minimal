APP_NAME="ТФОМС Краснодарского края"
APP_ENV=production
APP_KEY=base64:YOUR_APP_KEY_HERE
APP_DEBUG=false
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_LEVEL=warning

# SQLite Database
DB_CONNECTION=sqlite
DB_DATABASE=/var/www/database/database.sqlite

# Cache and Sessions
CACHE_DRIVER=file
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Queues
QUEUE_CONNECTION=database

# ЕСИА
ESIA_MINI_URL=http://localhost:3001
ESIA_CLIENT_ID=
ESIA_CLIENT_SECRET=
ESIA_REDIRECT_URI=http://localhost/virtual-reception/callback
